package algorithm;

/**
@author chenyankai
@Date	Jul 4, 2017

*/


public class BasicAlgo {
	private int graph[][]=null;//graph structure
	private int nodes[][]=null;//the tree nodes of each node
	private int core[]=null;

	public BasicAlgo(int graph[][],int nodes[][],int[] core){
		this.graph=graph;
		this.nodes=nodes;
		this.core=core;
	}
	

	
}
