package prep;

import java.io.File;
import java.net.MalformedURLException;
import java.net.URL;

import javax.net.ssl.HttpsURLConnection;

public class HTMLDownload {
	String url=null;
	String fileName=null;
	
	public HTMLDownload(String url,String fileName){
		this.url=url;
		this.fileName=fileName;
	}
	
	public int download(){
		
		try {
			URL u=new URL(url);
			HttpsURLConnection conn=(HttpsURLConnection)u.openConnection();
			conn.setConnectTimeout(downloadConfig.connectTime);
			conn.setReadTimeout(downloadConfig.connectTime);
			conn.setRequestProperty("User-Agent", "Mozilla/5.0 (compatible; MSIE 8.0; Windows NT 5.1; Trident/4.0)");

			
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
	}
	
	
	
	
}
