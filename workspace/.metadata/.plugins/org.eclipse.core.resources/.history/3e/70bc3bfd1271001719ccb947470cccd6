package algorithm.MineMaxPC;

import java.util.*;

/**
@author chenyankai
@Date	Jul 24, 2017
this is the data structure for profile tree node
*/
public class PNode {
	private String Attribute=null;
	private int ID=-1;
	private Set<PNode> subPNode=null;

	public PNode(int id){
		this.ID=id;
	}
	
	public void addPNode(PNode node){
		this.subPNode.add(node);
	}
	
}
