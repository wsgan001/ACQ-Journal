package algorithm;

import java.util.*;
import java.util.Map.Entry;

public class Test {

	public static void main(String[] args){
		

		Map<Integer, Set<Integer>> map = new HashMap<Integer,Set<Integer>>();
		for(int i=0;i<10;i++){
			Set<Integer> set = new HashSet<Integer>();
			set.add(1);
			set.add(2);
			map.put(i, set);
		}
		
		Iterator<Entry<Integer, Set<Integer>>> enIter = map.entrySet().iterator();
		while(enIter.hasNext()){
			Entry<Integer, Set<Integer>> entry = enIter.next();
			if(entry.getKey()==3){
				enIter.remove();
			}
			
		}

		
		
		
	}
	
	
	
}
