package algorithm.kwIndex.Query1;
import java.util.*;

/**
@author chenyankai
@Date	Aug 26, 2017

*/
public class pattern {
	Set<Integer> set = null;
	
	public pattern(Set<Integer> set){this.set = set;}
	
	@Override
	public int hashCode() {
		// TODO Auto-generated method stub
		Iterator<Integer> iter = set.iterator();
		int sum=0;
		while(iter.hasNext()) sum += iter.next(); 
		return sum;
	}
	
	public static void main(String[] args){
		
		Set<Integer> set1=new HashSet<Integer>();
		set1.add(1);
		pattern p1=new pattern(set1);
		
		Set<Integer> set2=new HashSet<Integer>();
		set1.add(2);
		pattern p2=new pattern(set2);
		
		System.out.println(set1.hashCode()+"   "+set2.hashCode()+"  "+set1.equals(set2));
		System.out.println(p1.hashCode()+"   "+p2.hashCode()+"  "+p1.equals(p2));

	}
	
	
}
